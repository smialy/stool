  "exports": {
    ".": {
      "require": "./dist/logging.js",
      "default": "./dist/logging.mjs"
    }
  },
  "devDependencies": {
    "@stool/micropack": "1.0.0",
    "@types/chai": "4.2.18",
    "@types/mocha": "8.2.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "eslint": "7.28.0",
    "karma": "6.3.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.1",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.1",
    "mocha": "9.0.0",
    "chai": "^4.3.4",
    "prettier": "2.3.1",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "files": [
    "dist/*.*",
    "README.md"
  ],
  "dependencies": {
  }
