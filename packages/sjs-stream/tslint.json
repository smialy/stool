{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark":[true, "single"],
        "max-classes-per-file": [false, 1],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "object-literal-sort-keys": [false],
        "prefer-for-of": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}