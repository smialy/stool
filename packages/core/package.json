{
  "name": "@stool/core",
  "version": "2.6.5",
  "description": "Core",
  "license": "MIT",
  "homepage": "https://github.com/smialy/stool#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smialy/stool.git"
  },
  "scripts": {
    "build": "micropack --include=zen-observable-ts",
    "dev": "npm run build -- --dev",
    "test": "mocha",
    "karma": "karma start karma.conf.cjs",
    "lint": "eslint src/*",
    "format": "prettier src tests --write",
    "prepare": "npm run build"
  },
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@stool/micropack": "0.6.2",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "chai": "^4.3.7",
    "eslint": "8.37.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-firefox-launcher": "2.1.2",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.3",
    "mocha": "10.2.0",
    "prettier": "^2.8.7",
    "ts-node": "10.9.1",
    "typescript": "4.4.3"
  },
  "files": [
    "dist/**/*.*",
    "README.md"
  ],
  "dependencies": {
    "zen-observable-ts": "^1.1.0"
  }
}
