{
  "name": "@stool/micropack",
  "version": "0.3.0",
  "description": "Simple bundler for tiny modules powered by rollup",
  "repository": {
    "type": "git",
    "url": "https://github.com/smialy/stool/tree/master/packages/micropack"
  },
  "bin": {
    "micropack": "src/cli.mjs"
  },
  "exports": {
    ".": {
      "require": "./dist/micropack.js",
      "import": "./src/micropack.mjs"
    }
  },
  "scripts": {
    "format": "prettier --write ./src",
    "lint": "eslint src/**/*.mjs"
  },
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@swc/core": "1.2.60",
    "commander": "7.2.0",
    "rollup": "2.51.1",
    "typescript": "4.3.2"
  },
  "files": [
    "src/**/*.*",
    "README.md"
  ],
  "keywords": [
    "bundle",
    "rollup",
    "micro library"
  ],
  "devDependencies": {
    "eslint": "^7.29.0",
    "prettier": "2.3.1"
  }
}
