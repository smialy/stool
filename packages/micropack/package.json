{
  "name": "@stool/micropack",
  "version": "0.2.1",
  "description": "Simple bundler for tiny modules, powered by rollup",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/smialy/stool/tree/master/packages/micropack"
  },
  "bin": {
    "micropack": "src/cli.mjs"
  },
  "exports": {
    ".": {
      "require": "./dist/micropack.js",
      "import": "./src/micropack.mjs"
    }
  },
  "scripts": {
    "format": "prettier --write ./src"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@modular-css/rollup": "^25.6.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "commander": "^5.1.0",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "src/**/*.*",
    "README.md"
  ],
  "keywords": [
    "bundle",
    "rollup",
    "micro library"
  ],
  "devDependencies": {
    "prettier": "^2.0.5"
  }
}
