{
  "name": "@stool/core",
  "version": "2.6.3",
  "description": "Core",
  "license": "MIT",
  "homepage": "https://github.com/smialy/stool#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smialy/stool.git"
  },
  "scripts": {
    "build": "micropack --tsconfig tsconfig.build.json",
    "test": "qunit --require ts-node/register tests/*.ts",
    "karma": "karma start --single-run",
    "lint": "tslint src/*",
    "prepare": "npm run build",
    "version": "npm run build"
  },
  "source": "src/index.ts",
  "main": "dist/core.js",
  "module": "dist/core.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/core.mjs",
      "require": "./dist/core.js"
    }
  },
  "devDependencies": {
    "@types/qunit": "^2.9.0",
    "@stool/micropack": "^0.0.4",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-qunit": "^4.0.0",
    "karma-typescript": "^5.0.0",
    "qunit": "^2.9.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/*.*",
    "README.md"
  ]
}
